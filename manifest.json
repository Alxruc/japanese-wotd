{
    "manifest_version": 3,
    "name": "Japenese Word Of The Day",
    "description": "Gives you a random Japanese word daily",
    "version": "1.0",

    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon-48.png"
    },

    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["word.js"]
      }
    ]
    
  }
  